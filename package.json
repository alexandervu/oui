{
  "name": "oui",
  "version": "1.0.0",
  "description": "A lightweight Vue 3 component library",
  "files": [
    "dist"
  ],
  "main": "./dist/oui.umd.js",
  "module": "./dist/oui.es.js",
  "exports": {
    ".": {
      "import": "./dist/oui.es.js",
      "require": "./dist/oui.umd.js"
    }
  },
  "unpkg": "./dist/oui.umd.js",
  "jsdelivr": "./dist/oui.umd.js",
  "scripts": {
    "lib:analyze": "vite build --config analyze.config.js",
    "lib:build": "vite build",
    "lib:publish": "npm publish --access public",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "lint": "eslint --ext .js,.vue src",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "keywords": [
    "vue",
    "ui",
    "oui"
  ],
  "author": "Alexander Vu <alexander.vu.tuyet@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.16.5",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/compiler-sfc": "^3.2.26",
    "@vue/test-utils": "^2.0.0-rc.17",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^27.4.5",
    "eslint": "^8.4.1",
    "eslint-plugin-vue": "^8.2.0",
    "jest": "^27.4.5",
    "postcss": "^8.4.5",
    "postcss-nested": "^5.0.6",
    "rollup-plugin-visualizer": "^5.5.2",
    "vite": "^2.7.2",
    "vitepress": "^0.20.5"
  },
  "dependencies": {
    "mitt": "^3.0.0",
    "tabbable": "^5.2.1",
    "tippy.js": "^6.3.7"
  }
}
